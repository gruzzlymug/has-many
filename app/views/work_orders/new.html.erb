<h1>New Work Order</h1>
<%= form_for @work_order do |f| %>
  <%= f.label "Tech: " %>
  <%= f.select :technician_id, Technician.all.map {|t| [t.name, t.id]} %>
  <br/>
  <%= f.label "Customer: " %>
  <%= f.select :customer_id, Customer.all.map {|c| [c.name, c.id]} %>
  <br/>
  <%= f.label :title %>:
  <%= f.text_field :title %>
  <br/>
  <%= f.submit %>
<% end %>
